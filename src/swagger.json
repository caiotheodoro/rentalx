{
    "openapi": "3.0.0",
    "info": {
        "title": "RentX API",
        "description": "This is an API Rent",
        "version": "1.0.0",
        "contact": {
            "name": "RentalX",
            "url": "https://rentalx.com",
            "email": "caiotheodoro12345@gmail.com"
        }
    },
    "paths": {
        "/categories": {
            "post": {
                "tags": [
                    "Categories"
                ],
                "summary": "Create a new category",
                "description": "Create a new category",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "description": "Category name",
                                        "example": "Category name  sample"
                                    },
                                    "description": {
                                        "type": "string",
                                        "description": "Category description",
                                        "example": "Category description  sample"
                                    }
                                }
                            }
                           
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Category created"
                    },
                    "400": {
                        "description": "Invalid category object"
                       
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            },
            "get": {
                "tags": [
                    "Categories"
                ],
                "summary": "List all categories",
                "description": "List all categories",
                "responses": {
                    "200": {
                        "description": "Categories list",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "format": "int64",
                                                "description": "Category id",
                                                "example": 1
                                            },
                                            "name": {
                                                "type": "string",
                                                "description": "Category name",
                                                "example": "Category name  sample"
                                            },
                                            "description": {
                                                "type": "string",
                                                "description": "Category description",
                                                "example": "Category description  sample"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/categories/import" : {
            "post": {
                "tags": [
                    "Categories"
                ],
                "summary": "Import categories",
                "description": "Import categories",
                "requestBody": {
                    "content": {
                        "application/octet-stream": {
                            "schema": {
                                "type": "string",
                                "properties": {
                                    "file": {
                                        "type": "string",
                                        "description": "File to import",
                                        "example": "File to import  sample"
                                    }
                                }
                            }
                           
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Categories imported"
                    },
                    "400": {
                        "description": "Invalid category object"
                       
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/specifications": {
            "post": {
                "tags": [
                    "Specifications"
                ],
                "summary": "Create a new specification",
                "description": "Create a new specification",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/definitions/Specification"
                            }
                           
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Specification created"
                    },
                    "400": {
                        "description": "Invalid specification object"
                       
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            },
            "get": {
                "tags": [
                    "Specifications"
                ],
                "summary": "List all specifications",
                "description": "List all specifications",
                "responses": {
                    "200": {
                        "description": "Specifications list",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "format": "int64",
                                                "description": "Specification id",
                                                "example": 1
                                            },
                                            "name": {
                                                "type": "string",
                                                "description": "Specification name",
                                                "example": "Specification name  sample"
                                            },
                                            "description": {
                                                "type": "string",
                                                "description": "Specification description",
                                                "example": "Specification description  sample"
                                            },
                                            "created_at": {
                                                "type": "string",
                                                "description": "Specification created at",
                                                "example": "2020-06-01T00:00:00.000Z"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "Specification": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Specification name",
                    "example": "Specification name  sample"
                },
                "description": {
                    "type": "string",
                    "description": "Specification description",
                    "example": "Specification description  sample"
                }
            }
        }
    }
}